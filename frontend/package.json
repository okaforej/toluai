    "@mui/material": "^7.3.1",
    "@mui/x-data-grid": "^8.10.0",
    "@mui/x-date-pickers": "^8.10.0",
    "@types/leaflet": "^1.9.20",
    "@types/lodash": "^4.17.20",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "axios": "^1.6.0",
    "clsx": "^2.1.1",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.0",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.4.0",
    "react-leaflet": "^4.2.1",
